{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon128.png",
      "default_popup": "popup.html",
      "default_title": "Merch Analytics"
   },
   "content_scripts": [ {
      "css": [ "bootstrap.min.css", "css.css" ],
      "js": [ "content.js", "jquery.js", "bootstrap.min.js", "Chart.js", "tablesort.min.js", "tablesort.number.js", "navscript.js", "popup.js" ],
      "matches": [ "https://*/MerchToolsTodaySales/*", "https://*/MerchToolsTwoWeeksSales/*", "https://*/MerchToolsAllMonthsSales/*", "https://*/MerchToolsEditor/*", "https://*/IndividualProductPage/*", "https://*/MerchAnalyticsSettings/*" ],
      "run_at": "document_end"
   } ],
   "description": "Analytics tool for Merch by Amazon",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "web_accessible_resources": [
       "options.html"
	],
   "manifest_version": 2,
   "name": "Merch Analytics",
   "permissions": [ "tabs", "activeTab", "contextMenus", "notifications", "storage", "*://*.amazon.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.0.1"
}
