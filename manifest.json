{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon128.png",
      "default_popup": "popup.html",
      "default_title": "Merch Analytics"
   },
   "content_scripts": [ {
      "css": [ "bootstrap.min.css", "css.css", "font-awesome.css", "daterangepicker.css" ],
      "js": [ "jquery.js", "bootstrap.min.js", "Chart.min.js", "tablesort.min.js", "tablesort.number.js", "moment.min.js", "daterangepicker.js", "popup.js", "navscript.js", "content.js"  ],
      "matches": [ "https://*/MerchAnalyticsTodaySales/*", "https://*/MerchAnalyticsTwoWeekSales/*", "https://*/MerchAnalyticsAllMonthsSales/*", "https://*/MerchAnalyticsProductManager/*", "https://*/IndividualProductPage/*", "https://*/MerchAnalyticsSettings/*" ],
      "run_at": "document_end"
   } ],
   "description": "Analytics tool for Merch by Amazon",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "web_accessible_resources": [
		"options.html",
		"fonts/fontawesome-webfont.woff",
		"fonts/fontawesome-webfont.woff2",
		"fonts/fontawesome-webfont.ttf",
		"fonts/*.woff",
		"fonts/*.woff2",
		"fonts/*.ttf",
		"*.ttf"
	],	
   "manifest_version": 2,
   "name": "Merch Analytics",
   "permissions": [ "tabs", "activeTab", "contextMenus", "notifications", "storage", "*://*.amazon.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.0.2"
}


